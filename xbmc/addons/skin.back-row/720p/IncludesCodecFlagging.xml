    <include name="recordlabel">
        <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>71</width>
            <height>48</height>
            <aspectratio>keep</aspectratio>
            <align>right</align>
            <aligny>center</aligny>
            <texture fallback="recordlabel fallback.png">special://skin/images/record labels/$INFO[ListItem.Property(Album_Label),,.png]</texture>
            <visible>Container.Content(Albums) + !stringcompare(ListItem.Label,..)</visible>
            <animation effect="fade" time="200">WindowClose</animation>
        </control>
    </include>
