            </control>
            <control type="scrollbar" id="61">
                <posx>32</posx>
                <posy>169</posy>
                <width>16</width>
                <height>440</height>
                <texturesliderbackground border="0,17.77,0,17.77">scrollbar-back.png</texturesliderbackground>
                <texturesliderbar border="0,17.77,0,17.77">scrollbar-front-focus.png</texturesliderbar>
                <texturesliderbarfocus border="0,17.77,0,17.77">scrollbar-front-focus.png</texturesliderbarfocus>
